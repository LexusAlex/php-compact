    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexsey Shmelev",
            "email": "alexsey_89@bk.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Compact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cli": "php bin/application.php --ansi",
        "phplint": "phplint",
        "cs-check": "phpcs",
        "cs-fixed": "phpcbf",
        "psalm": "psalm",
        "phpstan": "phpstan"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "overtrue/phplint": "^2.3",
        "vimeo/psalm": "^4.6",
        "psalm/plugin-phpunit": "^0.15.1",
        "lctrs/psalm-psr-container-plugin": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.81"
    },
    "require": {
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "filp/whoops": "^2.9",
        "php-di/php-di": "^6.3",
        "vlucas/phpdotenv": "^5.3",
        "laminas/laminas-config-aggregator": "^1.4",
        "ext-pdo": "*",
        "ext-json": "*",
        "symfony/console": "^5.2"
    }
}
